      <Text fx:id="text" layoutX="18.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" AnchorPane.leftAnchor="20.0" />
      <Text fx:id="userName" fill="#6b6b6b" layoutX="18.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" AnchorPane.leftAnchor="20.0">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Text fx:id="date" fill="#6b6b6b" layoutX="140.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="at xx:xx" AnchorPane.rightAnchor="20.0">
